{
  "name": "mimmi20/mezzio-sample-project",
  "description": "sample project for Mezzio Navigation",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-sample-project",
  "require": {
    "php": "~8.1.8 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "fig/http-message-util": "^1.1.5",
    "laminas/laminas-component-installer": "^3.5.0",
    "laminas/laminas-config": "^3.10.0",
    "laminas/laminas-config-aggregator": "^1.17.0",
    "laminas/laminas-diactoros": "^3.3.1",
    "laminas/laminas-form": "^3.21.0",
    "laminas/laminas-i18n": "^2.29.0",
    "laminas/laminas-i18n-resources": "^2.11.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "laminas/laminas-stdlib": "^3.20.0",
    "laminas/laminas-translator": "^1.1.0",
    "laminas/laminas-validator": "^2.64.1",
    "mezzio/mezzio": "^3.20.1",
    "mezzio/mezzio-helpers": "^5.17.0",
    "mezzio/mezzio-laminasrouter": "^3.10.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.16.0",
    "mezzio/mezzio-router": "^3.18.0",
    "mimmi20/laminas-form-element-group": "^3.0.10",
    "mimmi20/laminas-form-element-links": "^2.0.9",
    "mimmi20/laminas-form-element-paragraph": "^2.0.9",
    "mimmi20/laminasviewrenderer-bootstrap-form": "^2.0.8",
    "mimmi20/laminasviewrenderer-vite-url": "^1.0.5",
    "mimmi20/mezzio-generic-authorization": "^3.0.7",
    "mimmi20/mezzio-navigation": "^3.0.7",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^3.0.4",
    "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap": "^2.0.6",
    "mimmi20/mezzio-router-laminasrouter-factory": "^1.0.2",
    "mimmi20/mezzio-setlocale-middleware": "^1.0.3",
    "mimmi20/monolog-callbackfilterhandler": "^3.0.7",
    "mimmi20/monolog-factory": "^2.0.10",
    "mimmi20/monolog-streamformatter": "^3.1.4",
    "mimmi20/navigation-helper-acceptpage": "^2.1.4",
    "mimmi20/navigation-helper-containerparser": "^4.0.4",
    "mimmi20/navigation-helper-converttopages": "^2.1.6",
    "mimmi20/navigation-helper-findactive": "^2.1.4",
    "mimmi20/navigation-helper-findfromproperty": "^2.1.5",
    "mimmi20/navigation-helper-findroot": "^2.1.4",
    "mimmi20/navigation-helper-htmlify": "^4.0.4"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "filp/whoops": "^2.16.0",
    "infection/infection": "^0.28.1 || ^0.29.6",
    "mezzio/mezzio-tooling": "^2.9.0",
    "mimmi20/coding-standard": "^5.2.44",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.9",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.32",
    "rector/rector": "^1.2.10",
    "rector/type-perfect": "^1.0.0",
    "symplify/phpstan-rules": "^13.0.1",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^1.0.0",
    "tomasvotruba/unused-public": "^1.0.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/AppTest/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "laminas/laminas-component-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "platform": {
      "php": "8.1.8"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter",
        "mezzio/mezzio-laminasviewrenderer"
      ]
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "mezzio": "mezzio --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "php -S 0.0.0.0:8080 -t public/"
    ],
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
