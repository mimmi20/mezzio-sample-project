{
  "name": "mimmi20/mezzio-sample-project",
  "type": "library",
  "description": "sample project for Mezzio Navigation",
  "homepage": "https://github.com/mimmi20/mezzio-sample-project",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "laminas/laminas-component-installer": "^2.5.0",
    "laminas/laminas-config": "^3.5.0",
    "laminas/laminas-config-aggregator": "^1.5.0",
    "laminas/laminas-diactoros": "^2.6.0",
    "laminas/laminas-form": "^3.0.1",
    "laminas/laminas-i18n": "^2.11.1",
    "laminas/laminas-navigation": "^2.11.1",
    "laminas/laminas-servicemanager": "^3.7.0",
    "laminas/laminas-stdlib": "^3.5.0",
    "laminas/laminas-zendframework-bridge": "^1.3.0",
    "mezzio/mezzio": "^3.6.0",
    "mezzio/mezzio-helpers": "^5.6.0",
    "mezzio/mezzio-laminasrouter": "^3.1.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.3.0",
    "mimmi20/json-class": "^3.0.4",
    "mimmi20/laminas-form-element-group": "^2.0.0",
    "mimmi20/laminas-form-element-links": "^1.0.3",
    "mimmi20/laminas-form-element-paragraph": "^1.0.3",
    "mimmi20/mezzio-form-laminasviewrenderer-bootstrap": "^1.0.3",
    "mimmi20/mezzio-generic-authorization": "^2.0",
    "mimmi20/mezzio-navigation": "^2.0.6",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "dev-master",
    "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap": "dev-master",
    "mimmi20/monolog-laminas-factory": "^1.0.4",
    "mimmi20/navigation-helper-acceptpage": "dev-master",
    "mimmi20/navigation-helper-containerparser": "dev-master",
    "mimmi20/navigation-helper-converttopages": "dev-master",
    "mimmi20/navigation-helper-findactive": "dev-master",
    "mimmi20/navigation-helper-findfromproperty": "dev-master",
    "mimmi20/navigation-helper-findroot": "dev-master",
    "mimmi20/navigation-helper-htmlify": "dev-master"
  },
  "require-dev": {
    "filp/whoops": "^2.14.4",
    "laminas/laminas-dependency-plugin": "^2.1.2",
    "mezzio/mezzio-tooling": "^2.0.0",
    "mimmi20/coding-standard": "^2.4.2",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.94",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.22",
    "phpunit/phpunit": "^9.5.8"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter",
        "mezzio/mezzio-laminasviewrenderer"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/AppTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "mezzio": "mezzio --ansi",
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
