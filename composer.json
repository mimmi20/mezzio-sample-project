{
  "name": "mimmi20/mezzio-sample-project",
  "type": "library",
  "description": "sample project for Mezzio Navigation",
  "homepage": "https://github.com/mimmi20/mezzio-sample-project",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "fig/http-message-util": "^1.1.5",
    "laminas/laminas-component-installer": "^3.4.0",
    "laminas/laminas-config": "^3.9.0",
    "laminas/laminas-config-aggregator": "^1.14.0",
    "laminas/laminas-diactoros": "^3.3.0",
    "laminas/laminas-form": "^3.17.0",
    "laminas/laminas-i18n": "^2.24.1",
    "laminas/laminas-servicemanager": "^3.22.1",
    "laminas/laminas-stdlib": "^3.18.0",
    "mezzio/mezzio": "^3.18.0",
    "mezzio/mezzio-helpers": "^5.16.0",
    "mezzio/mezzio-laminasrouter": "^3.9.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.15.0",
    "mezzio/mezzio-router": "^3.17.0",
    "mimmi20/laminas-form-element-group": "^3.0.6",
    "mimmi20/laminas-form-element-links": "^2.0.5",
    "mimmi20/laminas-form-element-paragraph": "^2.0.5",
    "mimmi20/mezzio-form-laminasviewrenderer-bootstrap": "^2.0.2",
    "mimmi20/mezzio-generic-authorization": "^3.0.3",
    "mimmi20/mezzio-navigation": "^3.0.1",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^3.0.0",
    "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap": "^2.0.0",
    "mimmi20/monolog-callbackfilterhandler": "^3.0.4",
    "mimmi20/monolog-factory": "^2.0.6",
    "mimmi20/monolog-streamformatter": "^3.1.0",
    "mimmi20/navigation-helper-acceptpage": "^2.0.0",
    "mimmi20/navigation-helper-containerparser": "^4.0.0",
    "mimmi20/navigation-helper-converttopages": "^2.0.0",
    "mimmi20/navigation-helper-findactive": "^2.0.0",
    "mimmi20/navigation-helper-findfromproperty": "^2.0.0",
    "mimmi20/navigation-helper-findroot": "^2.0.0",
    "mimmi20/navigation-helper-htmlify": "^4.0.0"
  },
  "require-dev": {
    "filp/whoops": "^2.15.4",
    "mezzio/mezzio-tooling": "^2.9.0",
    "mimmi20/coding-standard": "^5.2.2",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.50",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpunit/phpunit": "^10.5.3",
    "rector/rector": "^0.18.12"
  },
  "config": {
    "platform": {
      "php": "8.1.8"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "laminas/laminas-component-installer": true,
      "laminas/laminas-dependency-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter",
        "mezzio/mezzio-laminasviewrenderer"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/AppTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "mezzio": "mezzio --ansi",
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
