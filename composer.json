{
  "name": "mimmi20/mezzio-sample-project",
  "description": "sample project for Mezzio Navigation",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-sample-project",
  "require": {
    "php": "~8.4.0 || ~8.5.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "fig/http-message-util": "^1.1.5",
    "laminas/laminas-component-installer": "^3.7.0",
    "laminas/laminas-config-aggregator": "^1.19.0",
    "laminas/laminas-diactoros": "^3.8.0",
    "laminas/laminas-filter": "^2.40.0",
    "laminas/laminas-form": "^3.24.2",
    "laminas/laminas-httphandlerrunner": "^2.13.0",
    "laminas/laminas-hydrator": "^4.18.0",
    "laminas/laminas-i18n": "^2.32.1",
    "laminas/laminas-i18n-resources": "^2.13.0",
    "laminas/laminas-inputfilter": "^2.35.0",
    "laminas/laminas-router": "^3.16.0",
    "laminas/laminas-servicemanager": "^3.24.0",
    "laminas/laminas-stdlib": "^3.21.0",
    "laminas/laminas-stratigility": "^4.3.0",
    "laminas/laminas-translator": "^1.3.0",
    "laminas/laminas-validator": "^2.64.2",
    "laminas/laminas-view": "^2.44.0",
    "mezzio/mezzio": "^3.27.0",
    "mezzio/mezzio-authentication": "^1.12.0",
    "mezzio/mezzio-helpers": "^5.20.0",
    "mezzio/mezzio-laminasrouter": "^3.12.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.19.0",
    "mezzio/mezzio-router": "^3.18.0",
    "mezzio/mezzio-template": "^2.12.0",
    "mimmi20/laminas-form-element-links": "^4.0.0",
    "mimmi20/laminas-form-element-paragraph": "^4.0.0",
    "mimmi20/laminasviewrenderer-bootstrap-form": "^3.0.3",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^3.1.0",
    "mimmi20/laminasviewrenderer-helper-partialrenderer": "^3.0.3",
    "mimmi20/laminasviewrenderer-vite-url": "^3.0.3",
    "mimmi20/mezzio-generic-authorization": "^4.0.0",
    "mimmi20/mezzio-navigation": "^5.0.0",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^7.0.0",
    "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap": "^5.0.0",
    "mimmi20/mezzio-router-laminasrouter-factory": "^2.0.0",
    "mimmi20/mezzio-setlocale-middleware": "^2.0.3",
    "mimmi20/monolog-callbackfilterhandler": "^4.0.2",
    "mimmi20/monolog-factory": "^3.0.0",
    "mimmi20/monolog-streamformatter": "^4.0.6",
    "monolog/monolog": "^3.10.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "psr/http-message": "^2.0",
    "psr/http-server-handler": "^1.0.2",
    "psr/log": "^3.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "filp/whoops": "^2.18.4",
    "infection/infection": "^0.32.2",
    "jbelien/phpstan-sarif-formatter": "^1.2.0",
    "mimmi20/coding-standard": "^7.0.18",
    "nikic/php-parser": "^5.7.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.33",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.5.4",
    "rector/rector": "^2.3.0",
    "rector/type-perfect": "^2.1.1",
    "shipmonk/composer-dependency-analyser": "^1.8.4",
    "symfony/process": "^8.0.3",
    "symplify/phpstan-rules": "^14.9.11",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.1.0",
    "tomasvotruba/unused-public": "^2.2.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/AppTest/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "laminas/laminas-component-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "platform": {
      "php": "8.4.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter",
        "mezzio/mezzio-laminasviewrenderer"
      ]
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "mezzio": "mezzio --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "php -S localhost:8081 -t public/ public/index.php -q"
    ],
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
