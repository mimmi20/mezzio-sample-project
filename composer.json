{
  "name": "mimmi20/mezzio-sample-project",
  "type": "library",
  "description": "sample project for Mezzio Navigation",
  "homepage": "https://github.com/mimmi20/mezzio-sample-project",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "laminas/laminas-component-installer": "^2.4.0",
    "laminas/laminas-config": "^3.5.0",
    "laminas/laminas-config-aggregator": "^1.5.0",
    "laminas/laminas-diactoros": "^2.5.1",
    "laminas/laminas-form": "^2.16.3",
    "laminas/laminas-i18n": "^2.11.1",
    "laminas/laminas-navigation": "^2.11.1",
    "laminas/laminas-servicemanager": "^3.6.4",
    "laminas/laminas-stdlib": "^3.3.1",
    "laminas/laminas-zendframework-bridge": "^1.2.0",
    "mezzio/mezzio": "^3.4.1",
    "mezzio/mezzio-helpers": "^5.6.0",
    "mezzio/mezzio-laminasrouter": "^3.1.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.3.0",
    "mimmi20/json-class": "^3.0.2",
    "mimmi20/mezzio-navigation": "^2.0.3",
    "mimmi20/mezzio-navigation-helpers": "^1.0.10",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^2.0.9",
    "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap": "^1.0.4"
  },
  "require-dev": {
    "filp/whoops": "^2.12.1",
    "infection/infection": "^0.21.5",
    "mezzio/mezzio-tooling": "^1.3.1",
    "mimmi20/coding-standard": "^2.1.3",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.87",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter",
        "mezzio/mezzio-laminasviewrenderer"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/AppTest/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "mezzio": "mezzio --ansi",
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
